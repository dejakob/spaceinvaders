<div class="relative">
    <article class="m10 rel">
        <h1 id="phoneTitle">Space<br />invaders</h1>
        <div class="overlay error" rt-if="this.showError">Someone already took your place. Sadly enough, but life is for the quick!</div>
    </article>
    <aside id="fixedButtons" class="{onlyTwitter ? 'onlyTwitter' : ''}">
        <button onClick={this.handleTwitterButton.bind(this)} class="twitter click"><i class="fa fa-twitter"></i> Connect with Twitter</button>
        <button onClick={this.handlePlayButton.bind(this)} class="click">Just play</button>
    </aside>
</div>